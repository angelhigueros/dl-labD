(* La sección {header} es opcional *)

(* Definiendo números hexadecimales *)

(* Definiendo las letras *)
let letterh = ['A'-'F']
let lettersh = letterh+

(* Definiendo los números *)
let digite = ['0'-'9']
let digitse = digite+

(* Definiendo los números *)
let hexdigit = ((letterh)(digitse))|((digitse)(letterh))
